/**
 * get方法，对应get请求
 * @param {String} url [请求的url地址]
 * @param {Object} params [请求时携带的参数]
 */
export function get(url, params) {
  console.log(url,'77777')
  return new Promise((resolve, reject) => {
    axios
      .get(url, {
        params: params,
      })
      .then((res) => {
        resolve(res.data);
      })
      .catch((err) => {
        reject(err.data);
      });
  });
}
/**
 * post方法，对应post请求
 * @param {String} url [请求的url地址]
 * @param {Object} params [请求时携带的参数]
 */

export function  post({ url, data }) {
  return new Promise((resolve, reject) => {
    axios({
      url, // 必填 请求API。
      timeout: 10000,
      method: "post",
      data,
      headers: {
        "X-Requested-With": "XMLHttpRequest",
        "Content-Type": "application/json;charset=UTF-8",
        TwiAuth: token || "",
        Authorization: token || "",
        Twi_Client: "web",
      },
    }).then((res) => {
          resolve(res);
        }).catch((err) => {
          reject(err);
        });

  });
}

