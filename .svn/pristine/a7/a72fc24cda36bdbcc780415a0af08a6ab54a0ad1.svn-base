.select {
    &-box {
        font-size: .3rem;
        color: #8e8e93;
    }
    &-transform{
        transform: rotate(90deg);
     
    }
    &-transfor{
        transform: rotate(-90deg);
     
    }
    &-title {
        display: flex;
        align-items: center;
        position: relative;
        padding: 0 .1rem;
        border-bottom: 1px solid #ededed;
    }
    &-custom-title, &-custom-item {
        width: 100%; 
        height: 100%; 
        display: flex; 
        align-items: center;
    }
    &-list-hidden {
        display: none;
    }
    &-item {
        position: relative;
        align-items: center;
        display: flex;
        padding:  0 .1rem;
        border-bottom: 1px solid #ededed;
    }
    &-item:last-child {
        border: none;
    }
    // &-icon::before {
    //     position: absolute;
    //     left: 100%;
    //     top: 50%;
    //     transform: translate(-50%, 50%);
    //     font-size: .2rem !important;
    // }
}