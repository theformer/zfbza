$prefix: 'yykj-car-number-input';

.#{$prefix} {
    &-wrap {
        width: 100%;
        height: .44rem;
        display: flex;
        background: white;
        justify-content: space-between;
        align-items: center;
        font-size: .28rem;
    }
    &-car-number-list {
        position: relative;
        height: 100%;
        width: 100%;
        display: flex;
        align-items: center;
    }
    &-cursor {
        width: .02rem;
        // height: 80%;
        background: black;
        position: absolute;
        display: block;
        top: 50%;
        transform: translate(-50%, -50%);
    }
}