@charset "utf-8";

$pColor:'#50CAFF';
$wColor:'#FCAE4B';
$sColor:'#34C759';
$bgColor:"#f5f5f5";
$itemBgColor:'#fff';

@function vw($value) {
    @return $value / 750 * 100vw;
}

@function ft($value) {
    @return $value / 750 * 100vmin;
}

/* CSS Document */
html,
body,
ul,
li,
ol,
dl,
dd,
dt,
p,
h1,
h2,
h3,
h4,
h5,
h6,
form,
fieldset,
legend,
img {
    margin: 0;
    padding: 0;
}

fieldset,
c {
    border: none;
}

img {
    display: block;
}

address,
caption,
cite,
code,
dfn,
th,
var {
    font-style: normal;
    font-weight: normal;
}

ul,
ol,
li {
    list-style: none;
}

body {
    color: #333;
    font: 12px "PingFangSC-Regular", "Hiragino Sans GB", sans-serif;
    background: #fff;
}

a {
    color: #666;
    text-decoration: none;
}

/*清除浮动*/
.clear {
    clear: both
}

* {
    box-sizing: border-box
}

.container {
    min-height: 100vh;
    overflow-x: hidden;
}

::-webkit-scrollbar {
    width: 0;
    height: 0;
}

.after-border,
.after-border-all,
.before-border,
.before-border-all,
.after-right-border,
.am-navbar.am-navbar-light {
    position: relative;
}

.after-border:not(:last-child)::after {
    content: "";
    position: absolute;
    width: 200%;
    height: 1px;
    top: 100%;
    left: 0;
    transform: scale(0.5) translate(-50%, -100%);
    display: block;
    background: #EDEDED;
}

.after-border-all::after,
.am-navbar.am-navbar-light::after {
    content: "";
    position: absolute;
    width: 200%;
    height: 1px;
    top: 100%;
    left: 0;
    transform: scale(0.5) translate(-50%, -100%);
    display: block;
    background: #EDEDED;
}


.before-border:not(:first-child)::before {
    content: "";
    position: absolute;
    width: 200%;
    height: 1px;
    top: 0;
    left: 0;
    transform: scale(0.5) translate(-50%, 0);
    display: block;
    background: #EDEDED;
}

.before-border-all::before {
    content: "";
    position: absolute;
    width: 200%;
    height: 1px;
    top: 0;
    left: 0;
    transform: scale(0.5) translate(-50%, 0);
    display: block;
    background: #EDEDED;
}

.after-right-border:not(:last-child):after {
    content: "";
    display: block;
    width: 1px;
    height: 60%;
    position: absolute;
    left: 100%;
    top: 50%;
    transform: translate(-50%, -50%);
    background: #ededed;
}

.split {
    width: 100%;
    height: vw(20);
    background: #fafafa;
}

div,
ul,
li,
ol,
a {
    box-sizing: border-box;
}

